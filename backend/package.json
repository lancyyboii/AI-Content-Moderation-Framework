{
  "name": "ai-content-moderation-api",
  "version": "1.0.0",
  "description": "AI-powered content moderation API with Node.js, Express, in-memory storage, and Ollama",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "build": "npm install --production",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "docker:build": "docker build -t ai-content-moderation .",
    "docker:run": "docker run -p 3000:3000 --env-file .env ai-content-moderation"
  },
  "keywords": [
    "ai",
    "content-moderation",
    "express",
    "in-memory",
    "ollama",
    "api"
  ],
  "author": "Lance Cabanit (Lancyy)",
  "license": "MIT",
  "dependencies": {
    "archiver": "^6.0.1",
    "async": "^3.2.5",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "file-type": "^18.7.0",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^17.11.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "p-limit": "^4.0.0",
    "p-queue": "^7.4.1",
    "redis": "^4.6.10",
    "retry": "^0.13.1",
    "sanitize-html": "^2.11.0",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3",
    "terser": "^5.44.0",
    "wait-on": "^7.2.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
